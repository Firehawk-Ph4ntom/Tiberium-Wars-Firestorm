	<!-- Increased damage from 325 -->
	<WeaponTemplate
		id="GDIOrcaRocketWeaponWarhead"
		Name="GDIOrcaRocketWeaponWarhead"
		AntiMask="ANTI_AIRBORNE_VEHICLE ANTI_GROUND ANTI_AIRBORNE_INFANTRY"
		RadiusDamageAffects="ENEMIES NEUTRALS NOT_SIMILAR"
		ProjectileCollidesWith="ENEMIES NEUTRAL">
		<Nuggets>
			<DamageNugget
				Damage="425"
				Radius="8"
				DamageType="ROCKET"
				DeathType="EXPLODED"
				DamageFXType="GDI_MISSILE">
				<DamageScalarDetails
					Scalar="125%">
					<Filter
						Rule="NONE"
						Include="AIRCRAFT">
						<ExcludeThing>AlienHiveStalker</ExcludeThing>
					</Filter>
				</DamageScalarDetails>
				<DamageScalarDetails
					Scalar="150%">
					<Filter
						Rule="NONE"
						Include="VEHICLE"
						Exclude="HARVESTER"/>
				</DamageScalarDetails>
				<DamageScalarDetails
					Scalar="50%">
					<Filter
						Rule="NONE"
						Include="INFANTRY">
						<ExcludeThing>GDIJetpackTrooper</ExcludeThing>
						<ExcludeThing>AlienHiveStalker</ExcludeThing>
					</Filter>
				</DamageScalarDetails>
			</DamageNugget>
			<SuppressionNugget
				Radius="20"
				Suppression="50"
				DurationSeconds="5s"/>
		</Nuggets>
	</WeaponTemplate>

