	<!-- Increased damage from 350 -->
	<WeaponTemplate
		id="GDIGolemCannonWarhead"
		Name="GDIGolemCannonWarhead"
		ProjectileCollidesWith="ALLIES ENEMIES NEUTRAL STRUCTURES WALLS">
