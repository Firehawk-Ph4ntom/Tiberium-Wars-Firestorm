			<FXListBehavior id="ModuleTag_FXListRadBlue">
				<DieMuxData DeathTypes="POISONED_BETA"/>
				<Event Index="onDeath" FX="FX_InfRadGooBlue"/>
			</FXListBehavior>
			<FXListBehavior id="ModuleTag_FXListRadRed">
				<DieMuxData DeathTypes="EXTRA_5"/>
				<Event Index="onDeath" FX="FX_InfRadGooRed"/>
			</FXListBehavior>
			<WeaponSetUpdate id="ModuleTag_WeaponSetUpdate">
				<WeaponSlotHardpoint ID="1">
					<Weapon Ordering="PRIMARY_WEAPON" Template="NODAscendedArmCannon"/>
