				<OCL Type="INITIAL">
					<OCL>OCL_InfGibsChar</OCL>
				</OCL>
				<Sound Type="INITIAL" List="NOD_Awakened_VoiceDie"/>
				<DieMuxData DeathTypes="BURNED"/>
