<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Includes>
		<!-- Base Object -->
		<Include type="instance" source="NODBlackHand.xml"/>
	</Includes>
	<GameObject id="NODBlackTemplar" inheritFrom="NODBlackHand" EditorName="NODBlackTemplar" VoicePriority="10">
		<Behaviors>
			<WeaponSetUpdate id="ModuleTag_WeaponSetUpdate" xai:joinAction="Replace">
				<WeaponSlotHardpoint ID="1" AllowInterleavedFiring="true" InterleavedStyle="INTERLEAVE_RANDOM" WeaponChoiceCriteria="PREFER_MOST_DAMAGE">
					<Weapon Ordering="PRIMARY_WEAPON" Template="NODBlackTemplarFlamethrower"/>
					<Weapon Ordering="PRIMARY_WEAPON" Template="NODBlackTemplarPurifyingFlamethrower" ObjectStatus="WEAPON_UPGRADED_01"/>
					<Weapon Ordering="SECONDARY_WEAPON" Template="NODBlackTemplarGrenade"/>
					<Weapon Ordering="SECONDARY_WEAPON" Template="NODBlackTemplarPurifyingGrenade" ObjectStatus="WEAPON_UPGRADED_01"/>
				</WeaponSlotHardpoint>
			</WeaponSetUpdate>
		</Behaviors>
		<Body>
			<ActiveBody id="ModuleTag_Body" xai:joinAction="Replace" MaxHealth="400"/>
		</Body>
	</GameObject>
</AssetDeclaration>