			<ActiveBody id="ModuleTag_Body" MaxHealth="100.0" />
		</Body>
	</GameObject>
	<GameObject id="NODBlackHandPurifyingGrenadeProjectile" inheritFrom="NODBlackHandGrenadeProjectile" EditorName="NODBlackHandPurifyingGrenadeProjectile">
