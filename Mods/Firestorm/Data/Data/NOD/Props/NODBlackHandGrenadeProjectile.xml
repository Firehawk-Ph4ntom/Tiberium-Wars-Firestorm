			</DestroyDie>
			<BezierProjectile id="ModuleTag_BezierProjectileBehavior" FirstHeightMin="50" FirstHeightMax="50" SecondHeightMin="50" SecondHeightMax="50" FirstPercentIndent="20" SecondPercentIndent="90" DetonateCallsKill="true"/>
		</Behaviors>
