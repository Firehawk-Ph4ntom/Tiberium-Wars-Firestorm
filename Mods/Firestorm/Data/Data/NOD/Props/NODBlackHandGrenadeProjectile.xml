			<FireWeaponWhenDead xai:joinAction="Replace" id="ModuleTag_FinalWeapon" InitiallyActive="true" DeathWeapon="NODBlackHandGrenadePurifyingExplosion">
				<DieMuxData DeathTypes="ALL"/>
			</FireWeaponWhenDead>
		</Behaviors>
	</GameObject>
