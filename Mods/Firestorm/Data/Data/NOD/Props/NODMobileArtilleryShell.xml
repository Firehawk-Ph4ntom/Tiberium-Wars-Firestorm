			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior id="ModuleTag_FXList" xai:joinAction="Replace">
				<DieMuxData DeathTypes="ALL"/>
				<Event FX="FX_NODArtShellMissHeroic"/>
			</FXListBehavior>
		</Behaviors>
	</GameObject>
	<!-- Normal Upgraded Projectile -->
