<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<!-- Normal Cluster Bomb -->
	<GameObject id="NODAntiMaterielClusterBomb" Side="NOD" EditorSorting="MISC_MAN_MADE" KindOf="UNATTACKABLE NOT_AUTOACQUIRABLE NO_COLLIDE PASS_EXPERIENCE_TO_PRODUCER" EditorName="NODAntiMaterielClusterBomb">
		<DisplayName>NODAntiMaterielClusterBomb</DisplayName>
		<ArmorSet Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw id="ModuleTag_Draw">
				<ModelConditionState ParseCondStateType="PARSE_DEFAULT">
					<Model Name=""/>
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<ClusterBombUpdate id="ModuleTag_ClusterBombUpdate" WeaponName="NODAntiMaterielClusterBomblet" Radius="25.0" NumBomblets="5" MinDelay="0s" MaxDelay="0s" BombletFX="FX_NOD_AntiMateriel_CatalystExplosion"/>
			<LifetimeUpdate id="ModuleTag_LifetimeUpdate" MinLifetime="1s" MaxLifetime="1s" IgnoreProjectileState="true"/>
			<DestroyDie id="ModuleTag_Die">
				<DieMuxData DeathTypes="ALL"/>
			</DestroyDie>
		</Behaviors>
		<Body>
			<ActiveBody id="ModuleTag_Body" MaxHealth="100.0"/>
		</Body>
	</GameObject>
	<!-- Upgraded Cluster Bomb -->
	<GameObject id="NODAntiMaterielClusterBombBlue" inheritFrom="NODAntiMaterielClusterBomb" EditorName="NODAntiMaterielClusterBombBlue">
		<Behaviors>
			<ClusterBombUpdate id="ModuleTag_ClusterBombUpdate" xai:joinAction="Append" WeaponName="NODAntiMaterielClusterBombletBlue" BombletFX="FX_NOD_AntiMateriel_CatalystExplosionBlue"/>
		</Behaviors>
	</GameObject>
</AssetDeclaration>