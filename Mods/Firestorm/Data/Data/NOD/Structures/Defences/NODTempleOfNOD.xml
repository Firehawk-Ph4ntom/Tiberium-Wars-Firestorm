				<AnimationState ParseCondStateType="PARSE_NORMAL" ConditionsYes="PACKING">
					<Animation AnimationName="NBTPrimeMissle_IDLA" AnimationMode="ONCE" AnimationBlendTime="0"/>
				</AnimationState>
