<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<!-- Normal Projectile -->
	<GameObject id="AlienProtonProjectile" KindOf="PROJECTILE NO_COLLIDE HIDE_IF_FOGGED" Side="GDI" EditorName="AlienProtonProjectile" EditorSorting="SYSTEM">
		<ArmorSet Armor="NoArmor"/>
		<Draws>
			<ScriptedModelDraw id="ModuleTag_Draw">
				<ModelConditionState ParseCondStateType="PARSE_DEFAULT">
					<Model Name="FXPROTONSHELL"/>
					<ParticleSysBone BoneName="NONE" FXParticleSystemTemplate="TibShardProjectile_Trail_blue" FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior id="ModuleTag_FXList">
				<DieMuxData DeathTypes="ALL"/>
				<Event FX="FX_AlienAutoCannonHit"/>
			</FXListBehavior>
			<DestroyDie id="ModuleTag_Die">
				<DieMuxData DeathTypes="ALL"/>
			</DestroyDie>
			<BezierProjectile id="ModuleTag_BezierProjectile" FirstPercentIndent="33%" SecondPercentIndent="66%" FlightPathAdjustDistPerSecond="300" CurveFlattenMinDist="300" DetonateCallsKill="true" OrientToFlightPath="true"/>
		</Behaviors>
		<Body>
			<ActiveBody id="ModuleTag_Body" MaxHealth="100"/>
		</Body>
	</GameObject>
	<!-- Heroic Projectile -->
	<GameObject id="AlienProtonProjectile_Veteran" inheritFrom="AlienProtonProjectile" EditorName="AlienProtonProjectile_Veteran">
		<Draws>
			<ScriptedModelDraw id="ModuleTag_Draw" xai:joinAction="Replace">
				<ModelConditionState ParseCondStateType="PARSE_DEFAULT">
					<Model Name="FXPROTONSHELL_VET"/>
					<ParticleSysBone BoneName="NONE" FXParticleSystemTemplate="TibShardProjectile_Trail_heroic" FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior id="ModuleTag_FXList" xai:joinAction="Replace">
				<DieMuxData DeathTypes="ALL"/>
				<Event FX="FX_AlienAutoCannonHitHeroic"/>
			</FXListBehavior>
		</Behaviors>
	</GameObject>
</AssetDeclaration>