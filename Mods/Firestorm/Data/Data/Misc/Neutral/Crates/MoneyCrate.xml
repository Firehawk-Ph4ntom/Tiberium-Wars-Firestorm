<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Includes>
		<!-- Base Object -->
		<Include type="instance" source="DATA:Firestorm\Data\Data\Misc\BaseObjects\BaseCrate.xml"/>
	</Includes>
	<GameObject id="MoneyCrate" inheritFrom="BaseCrate" SelectPortrait="Portrait_MoneyCrate" ButtonImage="Portrait_MoneyCrate" EditorName="CrateMoney" Description="Desc:MoneyCrate">
		<DisplayName xmlns:xai="uri:ea.com:eala:asset:instance" xai:joinAction="Replace">Name:MoneyCrate</DisplayName>
		<Draws>
			<ScriptedModelDraw id="ModuleTag_Draw">
				<ModelConditionState ParseCondStateType="PARSE_DEFAULT">
					<Model Name="NPCRATE_FS"/>
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<MoneyCrateCollide id="ModuleTag_CrateCollide" MoneyProvided="1000" />
			<UnitCrateCollide id="ModuleTag_CrateCollide2" UnitCount="1" UnitType="MoneyCrateDummy"/>
			<SlowDeath id="ModuleTag_Death" SinkDelay="3s" SinkRate="1.0" DestructionDelay="8s">
				<OCL Type="INITIAL">
					<OCL>OCL_CrateSpawn</OCL>
				</OCL>
			</SlowDeath>
		</Behaviors>
	</GameObject>
	<!-- GDI Money Crate -->
	<GameObject id="GDIMoneyCrate" inheritFrom="MoneyCrate" KindOf="PARACHUTABLE SELECTABLE IMMOBILE NOT_AUTOACQUIRABLE UNATTACKABLE CAN_CAST_REFLECTIONS CRATE STRUCTURE" EditorName="GDIMoneyCrate" Side="GDI" BuildCost="1500" BuildTime="5" Description="Desc:PlayerMoneyCrate" BuildCompletion="PLACED_BY_PLAYER" ProductionQueueType="OTHER_STRUCTURE" BuildPlacementTypeFlag="OTHER_STRUCTURE" UnitCategory="STRUCTURE">
		<Behaviors>
			<MoneyCrateCollide id="ModuleTag_CrateCollide" xai:joinAction="Replace" MoneyProvided="1500" />
		</Behaviors>
	</GameObject>
	<!-- NOD Money Crate -->
	<GameObject id="NODMoneyCrate" inheritFrom="GDIMoneyCrate" EditorName="NODMoneyCrate" Side="NOD"/>
	<!-- Alien Money Crate -->
	<GameObject id="AlienMoneyCrate" inheritFrom="GDIMoneyCrate" EditorName="AlienMoneyCrate" Side="ALIEN"/>
</AssetDeclaration>