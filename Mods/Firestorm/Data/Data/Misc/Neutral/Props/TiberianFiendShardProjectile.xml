<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<!-- Normal Projectile -->
	<GameObject id="TiberianFiendShardProjectile" KindOf="PROJECTILE NO_COLLIDE HIDE_IF_FOGGED" Side="Neutral" EditorName="TiberianFiendShardProjectile" EditorSorting="SYSTEM">
		<ArmorSet Armor="NoArmor"/>
		<Draws>
			<ScriptedModelDraw id="ModuleTag_Draw">
				<ModelConditionState ParseCondStateType="PARSE_DEFAULT">
					<Model Name="TIBSHARDPROJECTILE_GREEN"/>
					<ParticleSysBone BoneName="NONE" FXParticleSystemTemplate="TibShardProjectile_Trail" FollowBone="true"/>
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior id="ModuleTag_FXList">
				<DieMuxData DeathTypes="ALL"/>
				<Event FX="FX_AlienTibShardHit"/>
			</FXListBehavior>
			<DestroyDie id="ModuleTag_Die">
				<DieMuxData DeathTypes="ALL"/>
			</DestroyDie>
			<BezierProjectile id="ModuleTag_BezierProjectile" FirstHeightMin="10" FirstHeightMax="10" SecondHeightMin="10" SecondHeightMax="10" FirstPercentIndent="33%" SecondPercentIndent="66%" FlightPathAdjustDistPerSecond="50" CurveFlattenMinDist="100" SidewaysDrift="5" DetonateCallsKill="true" OrientToFlightPath="true"/>
		</Behaviors>
		<Body>
			<ActiveBody id="ModuleTag_Body" MaxHealth="100"/>
		</Body>
		<Geometry IsSmall="true">
			<Shape MajorRadius="0.8"/>
		</Geometry>
		<VisionInfo/>
	</GameObject>
