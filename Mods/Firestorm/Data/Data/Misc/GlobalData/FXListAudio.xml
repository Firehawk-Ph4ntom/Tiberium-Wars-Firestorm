	<FXList
		id="FX_BaseCivilianStructureNoGarrisonDestructible_Die">
		<NuggetList>
			<!-- this is the onDeath FXListBehavior for BaseCivilianStructureNoGarrisonDestructible structures -->
			<!-- below filters out building explosion die sounds for the specific building -->
			<Sound Value="NOD_Battleship_DieMS" StopIfPlayed="true">
				<SourceObjectFilter Rule="ANY">
					<IncludeThing>NOD_Battleship</IncludeThing>
				</SourceObjectFilter>
			</Sound>
			<Sound Value="NOD_DockCargoCrane_DieMS" StopIfPlayed="true">
				<SourceObjectFilter Rule="ANY">
					<IncludeThing>NOD_DockCargoLoadingCrane</IncludeThing>
				</SourceObjectFilter>
			</Sound>
			<Sound Value="LargeGenericBuilding_Die_MS2" StopIfPlayed="true">
				<SourceObjectFilter Rule="ANY">
					<IncludeThing>DCBParl</IncludeThing>
				</SourceObjectFilter>
			</Sound>
			<Sound Value="HumanFaction_MediumBuilding_DieMS" />
		</NuggetList>
	</FXList>

