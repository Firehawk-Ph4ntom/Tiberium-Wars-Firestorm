<?xml version="1.0" encoding="us-ascii"?>

<Behaviors xmlns="uri:ea.com:eala:asset">

	<AutoHealBehavior
		xmlns="uri:ea.com:eala:asset"
		id="ModuleTag_SelfRepairHealBehavior"
		HealingAmount="50"
		HealingDelay="1s"
		StartHealingDelay="2s"
		HealOnlyIfNotUnderAttack="true"
		HealOnlyIfNotInCombat="true">
		<TriggeredBy>Upgrade_SelfRepair</TriggeredBy>
	</AutoHealBehavior>

	<AudioLoopUpgrade
		id="ModuleTag_SelfRepairAudioLoop"
		SoundToPlay="GDI_SelfRepair_Upgrade">
		<TriggeredBy>Upgrade_SelfRepair</TriggeredBy>
	</AudioLoopUpgrade>

</Behaviors>